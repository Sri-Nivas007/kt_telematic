{
    "env": {
        "node": true,
        "es2020": true
    },
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 2020,
        "sourceType": "module"
    },
    "extends": ["eslint:recommended", "plugin:node/recommended"],
    "rules": {
        "no-console": "warn",
        "no-unused-vars": "warn"
    }
}
